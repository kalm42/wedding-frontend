{
  "name": "wedding-frontend",
  "private": true,
  "description": "The frontend for a wedding RSVP management system.",
  "version": "1.0.1",
  "author": "@kalm42 Kyle Melton",
  "dependencies": {
    "@sentry/browser": "^5.2.1",
    "apollo-boost": "^0.4.0",
    "cypress-testing-library": "^3.0.1",
    "date-fns": "^1.30.1",
    "dotenv": "^8.0.0",
    "gatsby": "^2.5.7",
    "gatsby-image": "^2.1.0",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-offline": "^2.1.1",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-sharp": "^2.0.37",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-source-filesystem": "^2.0.37",
    "gatsby-source-graphql": "^2.0.18",
    "gatsby-transformer-sharp": "^2.1.19",
    "graphql-tag": "^2.10.1",
    "isomorphic-fetch": "^2.2.1",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-apollo": "^2.5.5",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-stripe-checkout": "^2.6.3",
    "styled-components": "^4.2.0",
    "test-data-bot": "^0.8.0"
  },
  "devDependencies": {
    "cypress": "^3.3.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-html": "^5.0.5",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "husky": "^2.3.0",
    "is-ci-cli": "^1.1.1",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1",
    "start-server-and-test": "^1.9.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "format": "yarn prettier -- --write",
    "prettier": "prettier src/**/*.{js,jsx,json,yml,yaml,css,less,scss,md,graphql}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "validate": "yarn test:coverage && yarn test:e2e:run",
    "precommit": "lint-staged",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "pretest:e2e:run": "yarn build",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "test:e2e:run": "start-server-and-test serve http://localhost:8000 cy:run",
    "test:e2e:dev": "start-server-and-test dev http://localhost:8000 cy:open"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
